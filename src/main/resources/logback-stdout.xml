<?xml version="1.0" encoding="UTF-8"?>

<included>
    <appender name="HazelcastHealthMonitor" class="ch.qos.logback.core.ConsoleAppender">
        <encoder>
            <charset>UTF-8</charset>
            <pattern>%d{"yyyy-MM-dd'T'HH:mm:ss.SSSZ"} %m%n</pattern>
        </encoder>
    </appender>
    <appender name="Application" class="ch.qos.logback.core.ConsoleAppender">
        <withJansi>true</withJansi>
        <encoder>
            <charset>UTF-8</charset>
            <pattern>%d{"yyyy-MM-dd'T'HH:mm:ss.SSSZ"} %highlight(%-5level) %m [%c] [%t]%n%ex{5}</pattern>
        </encoder>
    </appender>
    <appender name="ApplicationAll" class="ch.qos.logback.core.helpers.NOPAppender"></appender>
</included>